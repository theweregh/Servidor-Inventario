{
  "name": "products",
  "version": "1.0.0",
  "main": "build/scr/Product.js",
  "scripts": {
    "dev": "npx ts-node-dev --respawn --transpile-only scr/Product.ts",
    "dev:debug": "npx ts-node-dev --inspect --respawn scr/Product.ts",
    "dev:local": "npm run dev -- --env-file .env.local",
    "prebuild": "npm run clean && npm run lint && npm run test",
    "build": "npm run build:compile",
    "build:compile": "tsc --build tsconfig.json",
    "start": "node build/scr/Product.js",
    "test": "jest --coverage --detectOpenHandles",
    "test:watch": "jest --watch --detectOpenHandles",
    "lint": "npx eslint . --fix",
    "lint:check": "npx eslint .",
    "lint:report": "npx eslint scr/ --format json > eslint-report.json",
    "format": "prettier --write scr/**/*.ts",
    "type-check": "tsc --noEmit",
    "audit": "npm audit --audit-level=moderate",
    "security": "npm run audit && npm run dep-check",
    "dep-check": "depcheck --ignores='@types/*,jest,typescript'",
    "clean": "rimraf build coverage reports",
    "complexity": "plato -r -d reports/complexity build/scr/"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.34.0",
    "globals": "^16.3.0",
    "jest": "^30.1.3",
    "jiti": "^2.5.1",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.10",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "depcheck": "^1.4.7",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "mongodb": "^6.18.0",
    "socket.io": "^4.8.1",
    "typhonjs-escomplex": "^0.1.0"
  }
}
